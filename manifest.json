{
  "manifest_version": 2,

  "name": "DNS Flusher",
  "description": "flush dns cache with single click",
  "version": "0.1",
  "author": "loveky",
  "icons": {
    "64": "icon64.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "background": {
      "scripts": [ "background.js" ]
   },
  "browser_action": {
    "default_icon": "icon64.png"
  },
  "permissions": [
    "tabs",
    "chrome://net-internals/#dns"
  ],
  "homepage_url": "http://www.jd.com"
}
